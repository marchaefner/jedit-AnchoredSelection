<?xml version="1.0"?>
<!DOCTYPE ACTIONS SYSTEM "actions.dtd">
<ACTIONS>
  <ACTION NAME="toggle-anchor-select" NO_RECORD="TRUE">
    <CODE>
      anchoredselection.Actions.toggleAnchoredSelectionEnabled(view);
    </CODE>
    <IS_SELECTED>
      return anchoredselection.Actions.isAnchoredSelectionEnabled(view);
    </IS_SELECTED>
  </ACTION>
</ACTIONS>
